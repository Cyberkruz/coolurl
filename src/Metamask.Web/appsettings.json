{
  "AppSettings": {
    "Hostname": "http://localhost:5000/"
  },
  "ConnectionStrings": {
    "SqlDatabaseConnection": "Server=127.0.0.1;Database=metamask;Username=postgres;Password=mysecretpassword"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error"
