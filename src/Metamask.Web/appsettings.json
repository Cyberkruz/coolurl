{
  "AppSettings": {
    "Hostname": "http://localhost:59734/"
  },
  "ConnectionStrings": {
    "SqlDatabaseConnection": "Server=127.0.0.1;Database=metamask;User=sa;Password=yourStrong(!)Password;MultipleActiveResultSets=true"
  },
  "Serilog": {
    "MinimumLevel": {
